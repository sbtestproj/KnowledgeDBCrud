<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.EntitiesEntity" table="entities" schema="public" catalog="example">
        <id name="entitiesId">
            <column name="entities_id" sql-type="bigint"/>
        </id>
        <property name="entityTypeId">
            <column name="entity_types_id" sql-type="bigint"/>
        </property>
        <property name="entityName">
            <column name="entity_name" sql-type="varchar(250)" length="250"/>
        </property>
    </class>
</hibernate-mapping>
