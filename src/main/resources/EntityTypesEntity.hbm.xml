<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="EntityTypes.EntityTypesEntity" table="entity_types" schema="public" catalog="example">
        <id name="entityTypesId">
            <column name="entity_types_id" sql-type="bigint"/>
        </id>
        <property name="entityTypeName">
            <column name="entity_type_name" sql-type="varchar(250)" length="250"/>
        </property>
    </class>
</hibernate-mapping>
