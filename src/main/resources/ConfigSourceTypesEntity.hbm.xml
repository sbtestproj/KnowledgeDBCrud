<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ConfigSourceTypes.ConfigSourceTypesEntity" table="config_source_types" schema="public"
           catalog="example">
        <id name="configSourceTypesId">
            <column name="config_source_types_id" sql-type="bigint"/>
        </id>
        <property name="configSourceTypeName">
            <column name="config_source_type_name" sql-type="varchar(250)" length="250"/>
        </property>
    </class>
</hibernate-mapping>
