<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="EntityModuleRelations.EntityModuleRelationsEntity" table="entity_module_relations" schema="public"
           catalog="example">
        <id name="id">
            <column name="id" sql-type="bigint"/>
        </id>
        <property name="entities_id">
            <column name="entities_id" sql-type="bigint"/>
        </property>
        <property name="modules_id">
            <column name="modules_id" sql-type="bigint"/>
        </property>
    </class>
</hibernate-mapping>
